{
  "name": "frontend",
  "version": "3.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bacali95/personal-website.git"
  },
  "bugs": {
    "url": "https://github.com/bacali95/personal-website/issues"
  },
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "config": "ts-node ./scripts/setenv.ts",
    "start": "yarn run config --environment=dev && ng serve --proxy-config proxy.conf.json",
    "build": "yarn run config --environment=prod && ng build --baseHref=/admin/",
    "build:prod": "yarn build --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && yarn test --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint frontend --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "yarn lint && yarn lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "yarn lint:ci",
    "release:changelog": "yarn conventional-changelog -- -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@angular/animations": "^11.0.4",
    "@angular/cdk": "^11.0.2",
    "@angular/common": "^11.0.4",
    "@angular/compiler": "^11.0.4",
    "@angular/core": "^11.0.4",
    "@angular/forms": "^11.0.4",
    "@angular/localize": "^11.0.4",
    "@angular/platform-browser": "^11.0.4",
    "@angular/platform-browser-dynamic": "^11.0.4",
    "@angular/router": "^11.0.4",
    "@bacali/ng2-smart-table": "1.7.0",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@nebular/auth": "6.2.1",
    "@nebular/bootstrap": "6.2.1",
    "@nebular/eva-icons": "6.2.1",
    "@nebular/security": "6.2.1",
    "@nebular/theme": "6.2.1",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "bootstrap": "^4.5.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.8.1",
    "eva-icons": "^1.1.3",
    "firebase": "^7.24.0",
    "intl": "^1.2.5",
    "ng2-completer": "^9.0.1",
    "ngx-image-cropper": "^3.3.2",
    "normalize.css": "^8.0.1",
    "pace-js": "^1.0.2",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.6.3",
    "typeface-exo": "^1.1.13",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.4",
    "@angular/cli": "^11.0.4",
    "@angular/compiler-cli": "^11.0.4",
    "@angular/language-service": "^11.0.4",
    "@babel/compat-data": "~7.12.7",
    "@babel/preset-env": "^7.12.10",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.8",
    "codelyzer": "^6.0.1",
    "conventional-changelog-cli": "^2.1.1",
    "dotenv": "^8.2.0",
    "husky": "^4.3.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-scss": "^3.18.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-language-service": "^0.9.9",
    "typescript": "4.0.5",
    "yargs": "^16.2.0"
  }
}
