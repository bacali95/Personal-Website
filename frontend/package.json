    "@nebular/security": "4.6.0",
    "@nebular/theme": "4.6.0",
    "@ng-bootstrap/ng-bootstrap": "^5.1.4",
    "bootstrap": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.1",
    "eva-icons": "^1.1.2",
    "intl": "^1.2.5",
    "ng2-completer": "^3.0.3",
    "ngx-image-cropper": "^2.0.2",
